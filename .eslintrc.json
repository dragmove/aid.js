{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "jquery": true,
    "amd": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": "off",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-extra-boolean-cast": "off",
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  }
}